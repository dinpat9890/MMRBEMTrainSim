	public static void main(String[] args) throws FindFailed {
		try {
			powerMenuStartup obj = new powerMenuStartup();

			obj.powerMenu();

			obj.smc();
			obj.trainBuilder();
			obj.userCreate();
			obj.userEdit();
			obj.userDelete();
			obj.instructor();
			obj.scenario();
			obj.copyScenario();
			obj.editScenario();
			obj.deleteScenario();

			obj.startSimulator();
			obj.environmentControl();

		} catch (IOException e) {
			System.out.println("An error occurred.");
			e.printStackTrace();
		}
	}
}