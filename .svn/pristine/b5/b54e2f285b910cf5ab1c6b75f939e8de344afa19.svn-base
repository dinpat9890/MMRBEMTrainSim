function cellInfo = PneumaticsSystem_Out_Vehicle(varargin) 
% PNEUMATICSSYSTEM_OUT_VEHICLE returns a cell array containing bus object information 
% 
% Optional Input: 'false' will suppress a call to Simulink.Bus.cellToObject 
%                 when the MATLAB file is executed. 
% The order of bus element attributes is as follows:
%   ElementName, Dimensions, DataType, SampleTime, Complexity, SamplingMode, DimensionsMode, Min, Max, DocUnits, Description 

suppressObject = false; 
if nargin == 1 && islogical(varargin{1}) && varargin{1} == false 
    suppressObject = true; 
elseif nargin > 1 
    error('Invalid input argument(s) encountered'); 
end 

cellInfo = { ... 
  { ... 
    'BD_PneumaticsSystem_Out_Vehicle', ... 
    '', ... 
    '', ... 
    'Auto', ... 
    '-1', {... 
{'AxleBrakes', 1, 'Bus: BD_PneumaticsSystem_Out_Vehicle_AxleBrakes', -1, 'real', 'Sample', 'Fixed', [], [], sprintf('N'), ''}; ...
{'Audio', 1, 'Bus: BD_PneumaticsSystem_Out_Vehicle_Audio', -1, 'real', 'Sample', 'Fixed', [], [], '', sprintf('Pneumatic System Audio Triggers')}; ...
{'Debug', 1, 'Bus: BD_PneumaticsSystem_Out_Vehicle_Debug', -1, 'real', 'Sample', 'Fixed', [], [], '', sprintf('Pneumatic System Pressures')}; ...
{'PressureGauges', 1, 'Bus: BD_PneumaticsSystem_Out_Vehicle_Gauges', -1, 'real', 'Sample', 'Fixed', [], [], '', sprintf('Pneumatic System Pressure Gauges')}; ...
{'PressureSwitches', 1, 'Bus: BD_PneumaticsSystem_Out_Vehicle_Switches', -1, 'real', 'Sample', 'Fixed', [], [], '', sprintf('Pneumatic System Pressures Switches')}; ...
{'PressureTransducers', 1, 'Bus: BD_PneumaticsSystem_Out_Vehicle_Transducers', -1, 'real', 'Sample', 'Fixed', [], [], '', sprintf('Pneumatic System Pressures Transducers')}; ...
{'TCMS', 1, 'Bus: BD_PneumaticsSystem_Out_Vehicle_TCMS', -1, 'real', 'Sample', 'Fixed', [], [], '', sprintf('Pneumatic System TCMS Outputs')}; ...
    } ...
  } ...
}'; 

if ~suppressObject 
    % Create bus objects in the MATLAB base workspace 
    Simulink.Bus.cellToObject(cellInfo) 
end 
