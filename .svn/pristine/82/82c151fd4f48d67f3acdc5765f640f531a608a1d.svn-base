function cellInfo = TCMSSystem_Wires_DoorsTrainWire(varargin) 
% TCMSSYSTEM_WIRES_DOORSTRAINWIRE returns a cell array containing bus object information 
% 
% Optional Input: 'false' will suppress a call to Simulink.Bus.cellToObject 
%                 when the MATLAB file is executed. 
% The order of bus element attributes is as follows:
%   ElementName, Dimensions, DataType, SampleTime, Complexity, SamplingMode, DimensionsMode, Min, Max, DocUnits, Description 

suppressObject = false; 
if nargin == 1 && islogical(varargin{1}) && varargin{1} == false 
    suppressObject = true; 
elseif nargin > 1 
    error('Invalid input argument(s) encountered'); 
end 

cellInfo = { ... 
  { ... 
    'BD_TCMSSystem_Wires_DoorsTrainWire', ... 
    '', ... 
    '', ... 
    'Auto', ... 
    '-1', {... 
{'LeftDoorOpenRequest', 1, 'double', -1, 'real', 'Sample', 'Fixed', [], [], sprintf('W'), sprintf('-1 Disabled\n0 - inf Power Limit in Watts')}; ...
{'RightDoorOpenRequest', 1, 'double', -1, 'real', 'Sample', 'Fixed', [], [], sprintf('W'), sprintf('-1 Disabled\n0 - inf Power Limit in Watts')}; ...
{'LeftDoorCloseRequest', 1, 'double', -1, 'real', 'Sample', 'Fixed', [], [], sprintf('W'), sprintf('-1 Disabled\n0 - inf Power Limit in Watts')}; ...
{'RightDoorCloseRequest', 1, 'double', -1, 'real', 'Sample', 'Fixed', [], [], sprintf('W'), sprintf('-1 Disabled\n0 - inf Power Limit in Watts')}; ...
    } ...
  } ...
}'; 

if ~suppressObject 
    % Create bus objects in the MATLAB base workspace 
    Simulink.Bus.cellToObject(cellInfo) 
end 
