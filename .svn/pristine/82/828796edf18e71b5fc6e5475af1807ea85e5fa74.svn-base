#include "PublicAnnouncement.h"

namespace PIS
{
namespace Audio
{

PublicAnnouncement::PublicAnnouncement(int type):
    m_type(type)
{
}

PublicAnnouncement::~PublicAnnouncement()
{
}

int PublicAnnouncement::GetType() const
{
    return m_type;
}

void PublicAnnouncement::SetType(int type)
{
    m_type = type;
}

const std::string& PublicAnnouncement::GetJingle() const
{
    return m_jingle;
}

void PublicAnnouncement::SetJingle(const std::string &jingle)
{
    m_jingle = jingle;
}

}
