package com.sydac.mmrbem.tcms.common.data;

/**
 * This class hold data for HVAC Control Mode Status. <br>
 * Note: Data can be hold as {@link Integer}
 */
public class VehicleHvacControlModeStatusDO
{

  private int hvacControlStatus;

  private int hvacControlFault;

  private int hvacControlIsolated;

  public int getHvacControlStatus()
  {
    return hvacControlStatus;
  }

  public void setHvacControlStatus(int hvacStatus)
  {
    this.hvacControlStatus = hvacStatus;
  }

  public int getHvacControlFault()
  {
    return hvacControlFault;
  }

  public void setHvacControlFault(int hvacFault)
  {
    this.hvacControlFault = hvacFault;
  }

  public int getHvacControlIsolated()
  {
    return hvacControlIsolated;
  }

  public void setHvacControlIsolated(int hvacIsolated)
  {
    this.hvacControlIsolated = hvacIsolated;
  }
}