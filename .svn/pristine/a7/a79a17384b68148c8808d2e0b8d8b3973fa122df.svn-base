package com.sydac.mmrbem.tcms.common.data;

/**
 * This class hold data for FSB Status. <br>
 * Note: Data can be hold as {@link Integer}
 */
public class VehicleEmergencyBrakeStatusDO
{

  /** The car index. */
  private int carIndex;

  private int eb1Energized;

  private int eb2Energized;

  /**
   * Gets the car index.
   *
   * @return the car index
   */
  public int getCarIndex()
  {
    return carIndex;
  }

  /**
   * Sets the car index.
   *
   * @param carIndex
   *          the new car index
   */
  public void setCarIndex(int carIndex)
  {
    this.carIndex = carIndex;
  }

  public int getEb1Energized()
  {
    return eb1Energized;
  }

  public void setEb1Energized(int eb1Energized)
  {
    this.eb1Energized = eb1Energized;
  }

  public int getEb2Energized()
  {
    return eb2Energized;
  }

  public void setEb2Energized(int eb2Energized)
  {
    this.eb2Energized = eb2Energized;
  }
}