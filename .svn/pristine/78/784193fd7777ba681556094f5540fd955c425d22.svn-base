package com.sydac.mmrbem.papis.common.data;

/**
 * SpecialInfoDetailModel acts as a buffer to store the data read from the XML
 * files,
 */

public class EmergencyInfoDetailModel
{

  private int emgInfoId;

  private String emgInfoCode;

  public EmergencyInfoDetailModel(int emgInfoId, String emgInfoCode)
  {
    super();
    this.emgInfoId = emgInfoId;
    this.emgInfoCode = emgInfoCode;
  }

  public EmergencyInfoDetailModel(EmergencyInfoDetailModel model)
  {
    super();
    this.emgInfoId = model.emgInfoId;
    this.emgInfoCode = model.emgInfoCode;
  }

  public EmergencyInfoDetailModel()
  {
    // do nothing
  }

  public int getEmgInfoId()
  {
    return emgInfoId;
  }

  public void setEmgInfoId(int emgInfoId)
  {
    this.emgInfoId = emgInfoId;
  }

  public String getEmgInfoCode()
  {
    return emgInfoCode;
  }

  public void setEmgInfoCode(String emgInfoCode)
  {
    this.emgInfoCode = emgInfoCode;
  }
}
