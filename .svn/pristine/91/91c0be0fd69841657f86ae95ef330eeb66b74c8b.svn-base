/******************************************************************************
 *                                                                             *
 *              Skip Station Setting Screen                 					*
 *                                                                             *
 ******************************************************************************/
 .skip-screen-list-grid-border {
	-fx-border-color: white;
	-fx-border-radius: 2.0px; 
 }
 
 .button.skip-list-buttons, .button.skip-list-buttons:unselected {
 	-fx-background-color: transparent;
	-fx-font-family: "Arial";
	-fx-text-alignment: center;
	-fx-background-radius: 6.0px;
	-fx-font-size: 14.0px;
	-fx-font-weight: bold;
	-fx-text-fill: white;
	-fx-wrap-text: true;
	-fx-alignment: center;
	-fx-padding: 0.0;
	-fx-border-color: white;
	-fx-border-width: 1.0px 0.0px 1.0px 0.0px;
	-fx-border-insets: 0.0px 0.0px 0.0px 0.0px;
	-fx-background-insets: 0.0, 0.0, 0.0, 0.0;
	-fx-background-repeat: no-repeat;
	-fx-background-position: center;
	-fx-background-size: 100.0%;
 }
 
 .button.skip-list-buttons:selected {
 	-fx-border-color: white;
	-fx-border-width: 2.0px 1.0px 2.0px 1.0px;
	-fx-background-color: rgb(11.0, 211.0, 138.0);
 }
 
 .skip-options-unselected {
	 -fx-text-fill: white;
	-fx-font-size: 20.0px;
	-fx-font-family: "Arial Narrow";
	-fx-alignment: center;
	-fx-background-color: transparent;
	-fx-border-color: white;
	-fx-border-width: 1.0px 0.0px 1.0px 0.0px;
 }
 
 .skip-options-selected {
	 -fx-text-fill: white;
	-fx-font-size: 20.0px;
	-fx-font-family: "Arial Narrow";
	-fx-alignment: center;
	-fx-background-color: rgb(11.0, 211.0, 138.0);
	-fx-border-color: white;
	-fx-border-width: 2.0px 1.0px 2.0px 1.0px;
 }
 
 .skip-screen-up-arrow {
 	-fx-background-image: url('../images/arrow_up.png');
	-fx-background-position: center;
	-fx-background-repeat: no-repeat;
	-fx-background-size: 24.0%;
