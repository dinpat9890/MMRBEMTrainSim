    -fx-border-color: white;
    -fx-border-width: 1 1 1 1;
   -fx-background-color: rgb(34.0, 42.0, 55.0);
}

 .button.passenger-alarm-display-button, .button.passenger-alarm-display-button:selected {
    -fx-text-fill: white;
