 ******************************************************************************/
.spc-emg-display-textbox-label {
	-fx-text-fill: black;
	-fx-font-size: 18.0px;
	-fx-font-family: "Arial Narrow";
	-fx-alignment: center;
	-fx-background-color: white;
}

.spc-emg-no-of-times-label {
	-fx-text-fill: white;
	-fx-font-size: 18.0px;
	-fx-font-family: "Arial Narrow";
	-fx-font-weight: Bold;
	-fx-alignment: center-left;
}

.spc-emg-display-no-of-times-textbox-label {
	-fx-text-fill: black;
	-fx-font-size: 18.0px;
	-fx-font-family: "Calibri";
	-fx-alignment: center-left;
	-fx-background-color: white;
}

.spc-emg-drop-down-arrow-icon {
	-fx-background-image: url('../images/blue_arrow.png');
 	-fx-background-position: center;
	-fx-background-repeat: no-repeat;
	-fx-background-size: 30.0%;
	-fx-background-color: rgb(220.0, 220.0, 220.0);
	-fx-background-insets: 0.0 0.0 0.0 0.0;
	-fx-border-color: rgb(20.0, 20.0, 20.0);
	-fx-border-width: 0.0 1.5 1.5 0.0;
	-fx-border-insets: 0.0 2.0 2.0 0.0;
}

.spc-no-of-times-options-lbl {
	-fx-text-fill: black;
	-fx-font-size: 14.0px;
	-fx-font-family: "Calibri";
	-fx-alignment: center;
	-fx-background-color: white;
	-fx-border-color: rgb(20.0, 20.0, 20.0);
	-fx-border-width: 0.5 0.0 0.0 0.0;
}

/******************************************************************************
 *                                                                             *
 *              Passenger Alarm Screen      								   *
 *                                                                             *
 ******************************************************************************/
 
 .passenger-alarm-options-grid-border {
 	-fx-border-color: white;
 	-fx-border-width: 1.0px;
 }
 .button-default-selected {
 	-fx-background-insets: 0.0, 0.0, 0.0, 0.0;
	-fx-background-radius: 6.0px;
	-fx-border-radius: 4.0px;
	-fx-font-size: 14.0px;
	-fx-font-family: "Arial";
	-fx-background-repeat: no-repeat;
	-fx-background-position: center;
	-fx-background-size: 100.0%;
	-fx-wrap-text: true;
	-fx-text-alignment: center;
	-fx-padding: 0.0;
	-fx-text-fill: white;
	-fx-background-color: rgb(11.0, 211.0, 138.0);
	-fx-border-width: 2.0px;
	-fx-border-color: white;
    -fx-background-size: 100.0%;
}
 .button {
	-fx-background-insets: 0.0, 0.0, 0.0, 0.0;
	-fx-background-radius: 6.0px;
	-fx-border-radius: 4.0px;
	-fx-font-size: 14.0px;
	-fx-font-family: "Arial";
	-fx-background-repeat: no-repeat;
	-fx-background-position: center;
	-fx-background-size: 100.0%;
	-fx-wrap-text: true;
	-fx-text-alignment: center;
	-fx-padding: 0.0;
}
 .button.footer-button, .button.footer-button:unselected {
   	-fx-background-color: rgb(11.0, 211.0, 138.0);
	-fx-font-family: "Arial";
	-fx-text-alignment: center;
	-fx-background-radius: 6.0px;
	-fx-font-size: 14.0px;
	-fx-font-weight: bold;
	-fx-text-fill: white;
    -fx-background-size: 100.0%;
}

.button.footer-button:armed, .button.footer-button:selected {
    -fx-text-fill: white;
	-fx-background-color: rgb(11.0, 211.0, 138.0);
	-fx-font-size: 14.0px;
	-fx-font-family: "Arial";
	-fx-alignment: center;
	-fx-border-width: 2.0px;
	-fx-border-color: white;
    -fx-background-size: 100.0%;
}

.button.footer-button:connected {
	-fx-background-color: rgb(255.0, 91.0, 80.0);
}

.button.footer-button:disconnected {
    -fx-background-color: rgb(11.0, 211.0, 138.0);
}

.flash-on {
	-fx-text-alignment: center;
	-fx-font-family: "Arial";
	-fx-text-fill: white;
	-fx-background-radius: 6.0px;
	-fx-alignment: center;
	-fx-font-size: 14.0px;
	-fx-font-weight: bold;
	-fx-background-color: rgb(255.0, 91.0, 80.0);
}

.gray-button{
    -fx-background-color: rgb(127.0, 127.0, 127.0);
    -fx-font-family: "Arial";
    -fx-text-alignment: center;
    -fx-background-radius: 6.0px;
    -fx-font-size: 14.0px;
    -fx-font-weight: bold;
    -fx-text-fill: white;
    -fx-wrap-text: true;
    -fx-alignment: center;
    -fx-padding: 0.0;
    -fx-background-insets: 0.0, 0.0, 0.0, 0.0;
    -fx-background-repeat: no-repeat;
    -fx-background-position: center;
    -fx-background-size: 100.0%;
}

.gray-button, .gray-button:unselected{
  -fx-border-width: 0px;
    -fx-border-color: transparent;  
} 

.gray-button:selected{
    -fx-border-width: 4.0px;
    -fx-border-color: white;
}
.green-button{
    -fx-background-color: rgb(11.0, 211.0, 138.0);
    -fx-font-family: "Arial";
    -fx-text-alignment: center;
    -fx-background-radius: 6.0px;
    -fx-font-size: 14.0px;
    -fx-font-weight: bold;
    -fx-text-fill: white;
    -fx-wrap-text: true;
    -fx-alignment: center;
    -fx-padding: 0.0;
    -fx-border-color: transparent;
    -fx-background-insets: 0.0, 0.0, 0.0, 0.0;
    -fx-background-repeat: no-repeat;
    -fx-background-position: center;
    -fx-background-size: 100.0%;
}
.green-button, .green-button:unselected, .green-button:flash_off{
  -fx-border-width: 0px;
    -fx-border-color: transparent;  
} 

.green-button:selected{
    -fx-border-width: 4.0px;
    -fx-border-color: white;
}
.green-button:flash_on{
    -fx-border-width:5.0px;
    -fx-background-color: rgb(255.0, 91.0, 80.0); 
}

.button.passenger-alarm-display-button, .button.passenger-alarm-display-button:default {
	-fx-text-fill: white;
