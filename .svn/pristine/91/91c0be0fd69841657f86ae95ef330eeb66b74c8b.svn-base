 }
 
.skip-screen-down-arrow {
 	-fx-background-image: url('../images/arrow_down.png');
	-fx-background-position: center;
	-fx-background-repeat: no-repeat;
	-fx-background-size: 24.0%;
	-fx-background-color: rgb(255.0, 91.0, 80.0);
 }
 
 .skip-screen-right-arrow {
 	-fx-background-image: url('../images/arrow_right.png');
 	-fx-background-position: center;
	-fx-background-repeat: no-repeat;
	-fx-background-size: 100.0%;
	-fx-background-color: rgb(11.0, 211.0, 138.0);
	-fx-background-radius: 6.0px;
