package com.sydac.mmrbem.tcms.common.data;

/**
 * This class hold data for Fire System Screen - FDU Status. <br>
 * Note: Data can be hold as {@link Integer}
 */
public class VehicleFDUStatusDO
{

  private int fduStatus;

  private int fduFault;

  public int getFduStatus()
  {
    return fduStatus;
  }

  public void setFduStatus(int fduStatus)
  {
    this.fduStatus = fduStatus;
  }

  public int getFduFault()
  {
    return fduFault;
  }

  public void setFduFault(int fduFault)
  {
    this.fduFault = fduFault;
  }
}