package com.sydac.mmrbem.tcms.common.data;

/**
 * This class hold data for Fire System Screen - Under Frame Fire Alarm Status.
 * <br>
 * Note: Data can be hold as {@link Integer}
 */
public class VehicleUFARStatusDO
{

  private int ufarStatus;

  private int ufarFaultStatus;

  public int getUfarStatus()
  {
    return ufarStatus;
  }

  public void setUfarStatus(int ufarStatus)
  {
    this.ufarStatus = ufarStatus;
  }

  public int getUfarFaultStatus()
  {
    return ufarFaultStatus;
  }

  public void setUfarFaultStatus(int ufarFaultStatus)
  {
    this.ufarFaultStatus = ufarFaultStatus;
  }
}