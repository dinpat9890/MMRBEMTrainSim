function cellInfo = SimIntegration_Cfg_Vehicle(varargin) 
% SIMINTEGRATION_CFG_VEHICLE returns a cell array containing bus object information 
% 
% Optional Input: 'false' will suppress a call to Simulink.Bus.cellToObject 
%                 when the MATLAB file is executed. 
% The order of bus element attributes is as follows:
%   ElementName, Dimensions, DataType, SampleTime, Complexity, SamplingMode, DimensionsMode, Min, Max, DocUnits, Description 

suppressObject = false; 
if nargin == 1 && islogical(varargin{1}) && varargin{1} == false 
    suppressObject = true; 
elseif nargin > 1 
    error('Invalid input argument(s) encountered'); 
end 

cellInfo = { ... 
  { ... 
    'BD_SimIntegration_Cfg_Vehicle', ... 
    '', ... 
    '', ... 
    'Auto', ... 
    '-1', {... 
{'Common', 1, 'Bus: BD_CommonTrain_Cfg_Vehicle', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'Location', 1, 'Bus: BD_SimIntegration_Cfg_Vehicle_Location', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'Equipment', 1, 'Bus: BD_SimIntegration_Cfg_Vehicle_Equipment', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'Dynamics', 1, 'Bus: BD_TrainDynamics_Cfg_Vehicle', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'Brakes', 1, 'Bus: BD_TrainBrakes_Cfg_Vehicle', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'Pneumatics', 1, 'Bus: BD_PneumaticsSystem_Cfg_Vehicle', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'Traction', 1, 'Bus: BD_TractionSystem_Cfg_Vehicle', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'Pantograph', 1, 'Bus: BD_PantographSystem_Cfg_Vehicle', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'Doors', 1, 'Bus: BD_DoorSystem_Cfg_Door', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'Electrical', 1, 'Bus: BD_ElectricalSystem_Cfg_Vehicle', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
    } ...
  } ...
}'; 

if ~suppressObject 
    % Create bus objects in the MATLAB base workspace 
    Simulink.Bus.cellToObject(cellInfo) 
end 
