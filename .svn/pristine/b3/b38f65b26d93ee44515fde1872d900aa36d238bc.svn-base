  public void setTriuAStatus(Integer triuastatus)
  {
    this.triuAStatus.set(triuastatus);
  }

  public void setOccPaStatus(Integer occpastatus)
  {
    this.occPaStatus.set(occpastatus);
  }

  public void setTriuBStatus(Integer triubstatus)
  {
    this.triuBStatus.set(triubstatus);
