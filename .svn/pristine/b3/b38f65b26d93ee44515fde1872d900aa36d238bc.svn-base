 *
 */
public class CabSignalPanelModel extends AbstractScreenModel
{
  private IntegerProperty triuAStatus = new SimpleIntegerProperty();

  private IntegerProperty occPaStatus = new SimpleIntegerProperty();

  private IntegerProperty triuBStatus = new SimpleIntegerProperty();

  private IntegerProperty tcmsAStatus = new SimpleIntegerProperty();

  private IntegerProperty tcmsBStatus = new SimpleIntegerProperty();

  private IntegerProperty remoteStatus = new SimpleIntegerProperty();

  private IntegerProperty cabAStatus = new SimpleIntegerProperty();

  private IntegerProperty cabBStatus = new SimpleIntegerProperty();

  private IntegerProperty paPisAStatus = new SimpleIntegerProperty();

  private IntegerProperty paPisBStatus = new SimpleIntegerProperty();

  private IntegerProperty trAStatus = new SimpleIntegerProperty();

  private IntegerProperty trBStatus = new SimpleIntegerProperty();

