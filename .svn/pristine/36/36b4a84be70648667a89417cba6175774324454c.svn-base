	private void smc() throws IOException {
		find_Click_Test(right_Screen, FilePathConstant.IMAGE_PATH_1 + "DefaultManager.PNG", "", "SMC Login", 1.0);
		find_Click_Test(right_Screen, FilePathConstant.IMAGE_PATH_1 + "SuperUser.PNG", "", "Login", 1.0);
		find_Click_Test(right_Screen, FilePathConstant.IMAGE_PATH_1 + "password.PNG", "password", "Login", 1.0);
		find_Click_Test(right_Screen, FilePathConstant.IMAGE_PATH_1 + "okbutton.PNG", "", "Login", 2.0);
	}

	private void find_Click_Test(Screen screen, String filepath, String type_text, String functionality,
			double waitTime) {
		try {
			screen.find(filepath);
			screen.click(filepath);
			if (!type_text.isEmpty())
				screen.type(type_text);
			screen.wait(waitTime);
			if (screen.exists(filepath) != null)
				System.out.println("\n****Test Case 2: Test case Passed****\n");
			else
				System.out.println("\n****Test Case 2: Test case Failed****\n");
		} catch (FindFailed e) {
			System.out.println(functionality);
		}
	}

