package com.sydac.mmrbem.tcms.common.data;

/**
 * This class hold data for ECN ALG Status. <br>
 * Note: Data can be hold as {@link Integer}
 */
public class VehicleALGStatusDO
{

  private int algStatus;

  private int algFault;

  public int getAlgStatus()
  {
    return algStatus;
  }

  public void setAlgStatus(int algStatus)
  {
    this.algStatus = algStatus;
  }

  public int getAlgFault()
  {
    return algFault;
  }

  public void setAlgFault(int algFault)
  {
    this.algFault = algFault;
  }
}