function cellInfo = DoorSystem_In_Emergency_Door(varargin) 
% DOORSYSTEM_IN_EMERGENCY_DOOR returns a cell array containing bus object information 
% 
% Optional Input: 'false' will suppress a call to Simulink.Bus.cellToObject 
%                 when the MATLAB file is executed. 
% The order of bus element attributes is as follows:
%   ElementName, Dimensions, DataType, SampleTime, Complexity, SamplingMode, DimensionsMode, Min, Max, DocUnits, Description 

suppressObject = false; 
if nargin == 1 && islogical(varargin{1}) && varargin{1} == false 
    suppressObject = true; 
elseif nargin > 1 
    error('Invalid input argument(s) encountered'); 
end 

cellInfo = { ... 
  { ... 
    'BD_DoorSystem_In_Emergency_Door', ... 
    '', ... 
    '', ... 
    'Auto', ... 
    '-1', {... 
{'RampHandle', 1, 'boolean', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'EmergencyDoorHandle', 1, 'boolean', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'DoorWinder', 1, 'double', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'RampWinder', 1, 'double', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'EmerDoorLatched', 1, 'boolean', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'EmerDoorOpenFault', 1, 'boolean', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'EmergencyDoorMechanicalUnlocked', 1, 'double', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'RampCoverLockingLever', 1, 'double', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'RampMiddleSupport', 1, 'boolean', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'EmergencyDoorGasSpring', 1, 'boolean', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'HasCab', 1, 'boolean', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
    } ...
  } ...
}'; 

if ~suppressObject 
    % Create bus objects in the MATLAB base workspace 
    Simulink.Bus.cellToObject(cellInfo) 
end 
