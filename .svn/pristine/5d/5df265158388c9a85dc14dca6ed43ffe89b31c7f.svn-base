package com.sydac.mmrbem.tcms.common.data;

/**
 * This class hold data for wsp status. <br>
 * Note: Data can be hold as {@link Integer}
 */
public class VehicleWspStatusDO
{

  /** The car index. */
  private int carIndex;

  private int wspActivatedState1;

  private int wspActivatedState2;

  /**
   * Gets the car index.
   *
   * @return the car index
   */
  public int getCarIndex()
  {
    return carIndex;
  }

  /**
   * Sets the car index.
   *
   * @param carIndex
   *          the new car index
   */
  public void setCarIndex(int carIndex)
  {
    this.carIndex = carIndex;
  }

  public int getWspActivatedState1()
  {
    return wspActivatedState1;
  }

  public void setWspActivatedState1(int wspActivatedState1)
  {
    this.wspActivatedState1 = wspActivatedState1;
  }

  public int getWspActivatedState2()
  {
    return wspActivatedState2;
  }

  public void setWspActivatedState2(int wspActivatedState2)
  {
    this.wspActivatedState2 = wspActivatedState2;
  }
}