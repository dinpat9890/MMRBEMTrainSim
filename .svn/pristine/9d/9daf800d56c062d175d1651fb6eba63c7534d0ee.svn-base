function cellInfo = SimIntegration_In_Train(varargin) 
% SIMINTEGRATION_IN_TRAIN returns a cell array containing bus object information 
% 
% Optional Input: 'false' will suppress a call to Simulink.Bus.cellToObject 
%                 when the MATLAB file is executed. 
% The order of bus element attributes is as follows:
%   ElementName, Dimensions, DataType, SampleTime, Complexity, SamplingMode, DimensionsMode, Min, Max, DocUnits, Description 

suppressObject = false; 
if nargin == 1 && islogical(varargin{1}) && varargin{1} == false 
    suppressObject = true; 
elseif nargin > 1 
    error('Invalid input argument(s) encountered'); 
end 

cellInfo = { ... 
  { ... 
    'BD_SimIntegration_In_Train', ... 
    '', ... 
    '', ... 
    'Auto', ... 
    '-1', {... 
{'Common', 1, 'Bus: BD_CommonTrain_In_Train', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'Overrides', 1, 'Bus: BD_SimIntegration_In_Train_Overrides', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'CBTCTuning', 1, 'Bus: BD_SimIntegration_In_Train_CBTC_Tuning', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'Faults', 1, 'Bus: BD_SimIntegration_Faults_Train', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
    } ...
  } ...
}'; 

if ~suppressObject 
    % Create bus objects in the MATLAB base workspace 
    Simulink.Bus.cellToObject(cellInfo) 
end 
