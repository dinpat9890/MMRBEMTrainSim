package com.sydac.mmrbem.tcms.common.data;

/**
 * This class hold data for Fire System Screen - External Smoke Status. <br>
 * Note: Data can be hold as {@link Integer}
 */
public class VehicleExternalSmokeStatusDO
{

  private int externalSmokeStatus;

  private int externalSmokeFaultStatus;

  public int getExternalSmokeStatus()
  {
    return externalSmokeStatus;
  }

  public void setExternalSmokeStatus(int externalSmokeStatus)
  {
    this.externalSmokeStatus = externalSmokeStatus;
  }

  public int getExternalSmokeFaultStatus()
  {
    return externalSmokeFaultStatus;
  }

  public void setExternalSmokeFaultStatus(int externalSmokeFaultStatus)
  {
    this.externalSmokeFaultStatus = externalSmokeFaultStatus;
  }
}