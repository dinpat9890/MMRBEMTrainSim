[Defaults]
System=System.ini
Database=Database.ini
Logging=Logging.ini
Hub=Hub.ini
Tuning=MotionBaseProfile.ini

[System.1]
Module Name=IWS-01-Motion Base Controller

[Motion Base Journal]
# Enables logging of the command and response interaction with the CKAS controller.
Enable Journal=0

[Comms]
# Request Frequency, in Hz. A value of zero means "on change". If unspecified, defaults to zero (0).
Request Frequency = 0

[Motion Base Hardware]
Destination Address=172.17.102.10
Destination Port = 1260
Local Port = 1260
Connection Timeout = 10.0

# Rate at which UDP packets are sent, in Hz. If unspecified, defaults to 100Hz.
Polling Rate = 100

[Logging]
Level=INFO