package com.sydac.mmrbem.tcms.common.state;

public enum VDUModeEnum
{
  VDU_TCMS_MODE(0, "TCMS"), VDU_HYBRID_MODE(1, "HYBRID"), VDU_CCTV_MODE(2, "CCTV");

  // VDU slection mode id
  private final int id;

  // VDU slection mode
  private final String mode;

  public int getId()
  {
    return id;
  }

  public String getMode()
  {
    return mode;
  }

  VDUModeEnum(int id, String mode)
  {
    this.id = id;
    this.mode = mode;
  }
}
