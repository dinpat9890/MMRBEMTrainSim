/** CSS styles for all the components is given here **/
.root {
    /** Can assign variable to common styles and use same for other components. **/
    -fx-background-color: rgb(192.0, 192.0, 192.0);
}

/** Gray Color used in various state **/
* {
    -fx-light-gray-color: rgb(192.0, 192.0, 192.0);
}

/** DMI background Color used in various state **/
* {
    -fx-dmi-background-color: rgb(28.0,40.0,65.0);
}

/** Black Color used in various state **/
* {
    -fx-black-color: rgb(0.0, 0.0, 0.0);
}

/** green Color used in various state **/
* {
    -fx-green-color: rgb(0.0, 255.0, 0.0);
}

/** Yellow Color used in various state **/
* {
    -fx-yellow-color: rgb(255.0, 255.0, 0.0);
}

/** red Color used in various state **/
* {
    -fx-red-color: rgb(255.0, 0.0, 0.0);
}

/** white Color used in various state **/
* {
    -fx-white-color: rgb(255.0, 255.0, 255.0);
}

/** dark gray Color used in various state **/
* {
    -fx-dark-gray-color: rgb(119.0, 136.0, 153.0);
}

/** gray Color used in various state **/
* {
    -fx-gray-color: rgb(195.0, 195.0, 197.0);
}

/** Dark Blue Color used in various state **/
* {
    -fx-dark-blue-color: rgb(6.0, 60.0, 120.0);
}

/** Blue Color used in various state **/
* {
    -fx-blue-color: rgb(0.0, 122.0, 255.0);
}

/** Bypass Normal state **/
* {
    -fx-bypass-normal-color: rgb(0.0, 0.0, 0.0);
}

/** Light blue color**/
* {
    -fx-light-blue-color: rgb(0.0, 122.0, 255.0);
}

/** Brown color**/
* {
    -fx-brown-color: brown;
}

/** red Color used in various state **/
* {
    -fx-orange-color: orange;
}

.main-panel-grid{
 -fx-background-color: -fx-dmi-background-color;
 -fx-border-width: 3.0;
 -fx-border-color: black;
 -fx-border-insets: 0.0;
 -fx-padding: 0.0 4.0 0.0 4.0;
}

.main-screen-label {
