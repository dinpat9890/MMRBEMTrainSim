.table-view {
    -fx-base: rgba(255.0, 255.0, 255.0, 0.0);
    -fx-control-inner-background: rgba(255.0, 255.0, 255.0, 0.0);
    -fx-background-color: rgba(255.0, 255.0, 255.0, 0.0);
    -fx-focus-traversable: false;
    -fx-padding: 0.0;
    -fx-insets: 0.0;
    -fx-border-insets: 0.0;
    -fx-wrap-text: false;
    -fx-border-color: -fx-light-gray-color;
    -fx-border-width: 1.0px;
}

.table-row-cell {
    -fx-background-color: rgba(255.0, 255.0, 255.0, 0.0);
    -fx-border-color: none;
    -fx-table-cell-border-color: rgba(255.0, 255.0, 255.0, 0.0);
    -fx-padding: 0.0;
    -fx-insets: 0.0;
    -fx-background-insets: 0.0;
    -fx-alignment: center;
    -fx-border-insets: 0.0;
    -fx-border-width: 0.0 0.0 0.0 0.0;
}


.table-row-cell .text {
       -fx-fill: -fx-light-gray-color;
       -fx-font-weight: bold;
       -fx-font-size : 19.0;
}

.table-view .scroll-bar * {
    -fx-min-width: 0.0;
    -fx-pref-width: 0.0;
    -fx-max-width: 0.0;
    -fx-min-height: 0.0;
    -fx-pref-height: 0.0;
    -fx-max-height: 0.0;
}

