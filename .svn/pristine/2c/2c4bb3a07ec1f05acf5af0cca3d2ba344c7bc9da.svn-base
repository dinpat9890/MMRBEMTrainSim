package com.sydac.mmrbem.tcms.common.data;

/**
 * This class hold data for BCG status. <br>
 * Note: Data can be hold as {@link Integer}
 */
public class VehicleBcgStatusDO
{

  /** The car index. */
  private int carIndex;

  private int bcgStatus1;

  private int bcgFault1;

  private int bcgIsolate1;

  private int bcgStatus2;

  private int bcgFault2;

  private int bcgIsolate2;

  /**
   * Gets the car index.
   *
   * @return the car index
   */
  public int getCarIndex()
  {
    return carIndex;
  }

  /**
   * Sets the car index.
   *
   * @param carIndex
   *          the new car index
   */
  public void setCarIndex(int carIndex)
  {
    this.carIndex = carIndex;
  }

  public int getBcgStatus1()
  {
    return bcgStatus1;
  }

  public void setBcgStatus1(int bcgStatus1)
  {
    this.bcgStatus1 = bcgStatus1;
  }

  public int getBcgFault1()
  {
    return bcgFault1;
  }

  public void setBcgFault1(int bcgFault1)
  {
    this.bcgFault1 = bcgFault1;
  }

  public int getBcgStatus2()
  {
    return bcgStatus2;
  }

  public void setBcgStatus2(int bcgStatus2)
  {
    this.bcgStatus2 = bcgStatus2;
  }

  public int getBcgFault2()
  {
    return bcgFault2;
  }

  public void setBcgFault2(int bcgFault2)
  {
    this.bcgFault2 = bcgFault2;
  }

  public int getBcgIsolate1()
  {
    return bcgIsolate1;
  }

  public void setBcgIsolate1(int bcgIsolate1)
  {
    this.bcgIsolate1 = bcgIsolate1;
  }

  public int getBcgIsolate2()
  {
    return bcgIsolate2;
  }

  public void setBcgIsolate2(int bcgIsolate2)
  {
    this.bcgIsolate2 = bcgIsolate2;
  }
}