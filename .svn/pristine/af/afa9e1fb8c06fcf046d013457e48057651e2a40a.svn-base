    <!-- 2ARS4401-M -->
    <route_case name="2ARS4401-M">
      <case>
        <actions>
          <mapped_state feature="2ARS4401_Signal" state="R" />
        </actions>
      </case>
      <case>
        <actions>
          <mapped_state feature="2ARS4401_Signal" state="V" />
        </actions>

        <track_circuit_requirement>
          <track_circuit_clear name="2AT4401" />
        </track_circuit_requirement>
      </case>
      <case>
