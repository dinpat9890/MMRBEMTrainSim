package com.sydac.mmrbem.tcms.fe.gui.model;

import com.sydac.mmrbem.cabscreens.fe.gui.model.AbstractScreenModel;

import javafx.beans.property.IntegerProperty;
import javafx.beans.property.SimpleIntegerProperty;

public class SpecialModesScreenModel extends AbstractScreenModel
{

  private IntegerProperty hteStatusProperty = new SimpleIntegerProperty();

  private IntegerProperty washingStatusProperty = new SimpleIntegerProperty();

  private IntegerProperty speedProperty = new SimpleIntegerProperty();

  private IntegerProperty shuntMinSpeedProperty = new SimpleIntegerProperty();

  private IntegerProperty shuntMaxSpeedProperty = new SimpleIntegerProperty();

  private IntegerProperty atpMaxSpeedProperty = new SimpleIntegerProperty();

  private IntegerProperty shuntModeStatusProperty = new SimpleIntegerProperty();

  private IntegerProperty atpLowSpeedStatusProperty = new SimpleIntegerProperty();

  private IntegerProperty shuntModeButtonStatusProperty = new SimpleIntegerProperty();

  private IntegerProperty atpModeSetButtonStatusProperty = new SimpleIntegerProperty();

  public SpecialModesScreenModel(int carIndex)
  {
    super(carIndex);
  }

  public IntegerProperty getHteStatusProperty()
  {
    return hteStatusProperty;
  }

  public void setHteStatusProperty(Integer hteStatusProperty)
  {
    this.hteStatusProperty.set(hteStatusProperty);
  }

  public IntegerProperty getWashingStatusProperty()
  {
    return washingStatusProperty;
  }

  public void setShuntModeStatusProperty(Integer shuntModeStatusProperty)
  {
    this.shuntModeStatusProperty.set(shuntModeStatusProperty);
  }

  public void setAtpLowSpeedStatusProperty(Integer atpLowSpeedStatusProperty)
  {
    this.atpLowSpeedStatusProperty.set(atpLowSpeedStatusProperty);
  }

  public void setWashingStatusProperty(Integer washingStatusProperty)
  {
    this.washingStatusProperty.set(washingStatusProperty);
  }

  public IntegerProperty getSpeedProperty()
  {
    return speedProperty;
  }

  public void setSpeedProperty(Integer speedProperty)
  {
    this.speedProperty.set(speedProperty);
  }

  public void setShuntMinSpeedProperty(Integer newValue)
  {
    this.shuntMinSpeedProperty.set(newValue);

  }

  public IntegerProperty getShuntMinSpeedProperty()
  {
    return shuntMinSpeedProperty;
  }

  public void setShuntMaxSpeedProperty(Integer newValue)
  {
    this.shuntMaxSpeedProperty.set(newValue);

  }

  public IntegerProperty getShuntMaxSpeedProperty()
  {
    return shuntMaxSpeedProperty;
  }

  public void setAtpMaxSpeedProperty(Integer newValue)
  {
    this.atpMaxSpeedProperty.set(newValue);

  }

  public IntegerProperty getAtpMaxSpeedProperty()
  {
    return atpMaxSpeedProperty;
  }

  public IntegerProperty getShuntModeStatusProperty()
  {
    return shuntModeStatusProperty;
  }

  public IntegerProperty getAtpLowSpeedStatusProperty()
  {
    return atpLowSpeedStatusProperty;
  }

  public IntegerProperty getShuntModeButtonStatusProperty()
  {
    return shuntModeButtonStatusProperty;
  }

  public void setShuntModeButtonStatusProperty(Integer shuntModeButtonStatusProperty)
  {
    this.shuntModeButtonStatusProperty.set(shuntModeButtonStatusProperty);
  }

  public IntegerProperty getAtpModeSetButtonStatusProperty()
  {
    return atpModeSetButtonStatusProperty;
  }

  public void setAtpModeSetButtonStatusProperty(Integer atpModeSetButtonStatusProperty)
  {
    this.atpModeSetButtonStatusProperty.set(atpModeSetButtonStatusProperty);
  }
}
