function cellInfo = TCMSSystem_Wires_TBTrainWire(varargin) 
% TCMSSYSTEM_WIRES_TBTRAINWIRE returns a cell array containing bus object information 
% 
% Optional Input: 'false' will suppress a call to Simulink.Bus.cellToObject 
%                 when the MATLAB file is executed. 
% The order of bus element attributes is as follows:
%   ElementName, Dimensions, DataType, SampleTime, Complexity, SamplingMode, DimensionsMode, Min, Max, DocUnits, Description 

suppressObject = false; 
if nargin == 1 && islogical(varargin{1}) && varargin{1} == false 
    suppressObject = true; 
elseif nargin > 1 
    error('Invalid input argument(s) encountered'); 
end 

cellInfo = { ... 
  { ... 
    'BD_TCMSSystem_Wires_TBTrainWire', ... 
    '', ... 
    '', ... 
    'Auto', ... 
    '-1', {... 
{'TractionPowerLimit', 1, 'double', -1, 'real', 'Sample', 'Fixed', [], [], sprintf('W'), sprintf('-1 Disabled\n0 - inf Power Limit in Watts')}; ...
{'EDBrakingPowerLimit', 1, 'double', -1, 'real', 'Sample', 'Fixed', [], [], sprintf('W'), sprintf('-1 Disabled\n0 - inf Power Limit in Watts')}; ...
{'EnableTractionPowerLimit', 1, 'double', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'EnableEDBrakingPowerLimit', 1, 'double', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'MaxTractionAccelerationTargetOverride', 1, 'double', -1, 'real', 'Sample', 'Fixed', [], [], sprintf('m/s/s'), ''}; ...
{'MaxBrakingDecelerationTargetOverride', 1, 'double', -1, 'real', 'Sample', 'Fixed', [], [], sprintf('m/s/s'), ''}; ...
{'EnableTractionTargetOverride', 1, 'double', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'EnableBrakingTargetOverride', 1, 'double', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'TractionCommand', 1, 'double', -1, 'real', 'Sample', 'Fixed', [], [], sprintf('%%'), ''}; ...
{'BrakingCommand', 1, 'double', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'HoldingBrakeStatus', 1, 'double', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'TCMSSpeedLimit', 1, 'double', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'Overspeed', 1, 'double', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'EstimatedMass', 1, 'double', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'HTERequired', 1, 'double', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'WashModeActive', 1, 'double', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'TractionForce', 1, 'double', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'ShuntModeActive', 1, 'double', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'ATPLowSpeedCutoutModeActive', 1, 'double', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
    } ...
  } ...
}'; 

if ~suppressObject 
    % Create bus objects in the MATLAB base workspace 
    Simulink.Bus.cellToObject(cellInfo) 
end 
