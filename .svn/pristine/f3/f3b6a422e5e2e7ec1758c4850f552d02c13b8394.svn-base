function cellInfo = SimIntegration_Prm(varargin) 
% SIMINTEGRATION_PRM returns a cell array containing bus object information 
% 
% Optional Input: 'false' will suppress a call to Simulink.Bus.cellToObject 
%                 when the MATLAB file is executed. 
% The order of bus element attributes is as follows:
%   ElementName, Dimensions, DataType, SampleTime, Complexity, SamplingMode, DimensionsMode, Min, Max, DocUnits, Description 

suppressObject = false; 
if nargin == 1 && islogical(varargin{1}) && varargin{1} == false 
    suppressObject = true; 
elseif nargin > 1 
    error('Invalid input argument(s) encountered'); 
end 

cellInfo = { ... 
  { ... 
    'BD_SimIntegration_Prm', ... 
    '', ... 
    '', ... 
    'Auto', ... 
    '-1', {... 
{'Dynamics', 1, 'BD_TrainDynamics_Prm', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'Brakes', 1, 'Bus: BD_TrainBrakes_Prm_Vehicle', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'Pneumatics', 1, 'Bus: BD_PneumaticsSystem_Prm', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'Traction', 1, 'Bus: BD_TractionSystem_Prm', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'Pantograph', 1, 'Bus: BD_PantographSystem_Prm', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'Doors', 1, 'Bus: BD_DoorSystem_Prm', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'Electrical', 1, 'Bus: BD_ElectricalSystem_Prm', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'TCMS', 1, 'Bus: BD_TCMSSystem_Prm', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'Coupling', 1, 'Bus: BD_CouplingSystem_Prm', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
    } ...
  } ...
}'; 

if ~suppressObject 
    % Create bus objects in the MATLAB base workspace 
    Simulink.Bus.cellToObject(cellInfo) 
end 
