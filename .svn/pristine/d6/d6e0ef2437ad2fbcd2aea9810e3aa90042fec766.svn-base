.root {
	-fx-background-color: -fx-black-color;
}
/** Gray Color used in various state **/
* {
    -fx-light-gray-color: rgb(192.0, 192.0, 192.0);
}

/** Light blue color**/
* {
    -fx-light-blue-color: rgb(0.0, 122.0, 255.0);
}

/** -fx-white-color Color used in various state **/
* {
    -fx-white-color: rgb(255.0, 255.0, 255.0);
}

/** Black Color used in various state **/
* {
    -fx-black-color: rgb(0.0, 0.0, 0.0);
}

/** green Color used in various state **/
* {
    -fx-green-color: rgb(0.0, 255.0, 0.0);
}

/** Yellow Color used in various state **/
* {
    -fx-yellow-color: rgb(255.0, 255.0, 0.0);
}

/** red Color used in various state **/
* {
    -fx-red-color: rgb(255.0, 0.0, 0.0);
}

.table-view {
    -fx-base: -fx-black-color;
    -fx-control-inner-background:-fx-black-color;
    -fx-background-color: -fx-black-color;
    -fx-focus-color: -fx-white-color;
    -fx-focus-traversable: false;
    -fx-padding: 0.0;
    -fx-insets: 0.0;
    -fx-border-insets: 0.0;
    -fx-wrap-text: true;
}

.table-row-cell {
    -fx-background-color: -fx-black-color;
    -fx-border-color: -fx-white-color;
    -fx-table-cell-border-color: -fx-white-color;
    -fx-padding: 0.0;
    -fx-insets: 0.0;
    -fx-background-insets: 0.0;
    -fx-alignment: center;
    -fx-border-insets: 0.0;
    -fx-border-width: 1.0 1.0 0.0 1.0;
    -fx-font-weight:bold;
}

.table-row-cell:last-row {
    -fx-border-width: 1.0 1.0 1.0 1.0;
}

.table-row-cell:selected {
    -fx-background-color: -fx-light-blue-color;
    -fx-text-fill: -fx-white-color;
}

/** CSS styles for all the components is given here **/
.common-bg {
    -fx-background-color: -fx-black-color;
    -fx-background-radius: 10.0px;
}

.common-grid-backround {
