package com.sydac.mmrbem.papis.common.data;

/**
 * LineDetailModel acts as a buffer to store the data read from the XML files,
 * later same is used to populate the FE.
 */
public class MultiplePecuCallModel
{
  private int pecuId;

  private String pecuName;

 

  public MultiplePecuCallModel(MultiplePecuCallModel model)
  {
    this.pecuId = model.pecuId;
    this.pecuName = model.pecuName;
    
  }

  public MultiplePecuCallModel()
  {
    // do nothing
  }

  public MultiplePecuCallModel(int lineId2, String lineName2)
  {
    this.pecuId = lineId2;
    this.pecuName = lineName2;
    
  }

  public int getPecuId()
  {
    return pecuId;
  }

  public void setPecuId(int pecu)
  {
    this.pecuId = pecu;
  }

  public String getPecuName()
  {
    return pecuName;
  }

  public void setPecuName(String pecuName)
  {
    this.pecuName = pecuName;
  }

  
}
