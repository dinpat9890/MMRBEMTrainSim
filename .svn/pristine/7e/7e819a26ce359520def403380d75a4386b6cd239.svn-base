function cellInfo = DoorSystem_In_Door_Faults(varargin) 
% DOORSYSTEM_IN_DOOR_FAULTS returns a cell array containing bus object information 
% 
% Optional Input: 'false' will suppress a call to Simulink.Bus.cellToObject 
%                 when the MATLAB file is executed. 
% The order of bus element attributes is as follows:
%   ElementName, Dimensions, DataType, SampleTime, Complexity, SamplingMode, DimensionsMode, Min, Max, DocUnits, Description 

suppressObject = false; 
if nargin == 1 && islogical(varargin{1}) && varargin{1} == false 
    suppressObject = true; 
elseif nargin > 1 
    error('Invalid input argument(s) encountered'); 
end 

cellInfo = { ... 
  { ... 
    'BD_DoorSystem_In_Door_Faults', ... 
    '', ... 
    '', ... 
    'Auto', ... 
    '-1', {... 
{'ObjectObstructingDoorWhileClosing', 1, 'double', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'ObjectObstructingDoorWhileOpening', 1, 'double', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'DoorFailToClose', 1, 'double', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'DoorFailToOpen', 1, 'double', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'DoorMotorCircuitFailure', 1, 'double', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'DoorFailedToLock', 1, 'double', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'DoorFailedToUnlock', 1, 'double', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'DoorUnexpectedUnlocking', 1, 'double', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'DoorDCUFailure', 1, 'double', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'DCSRFailure', 1, 'double', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'DCSLFailure', 1, 'double', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'DLSRFailure', 1, 'double', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'DLSLFailure', 1, 'double', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'DoorMajorFault', 1, 'double', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'DoorUnauthorisedOpen', 1, 'double', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'UnexpectedDoorOpeningDetectedByCC', 1, 'double', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'EmergencyDoorMechanicalUnlocked', 1, 'double', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'DoorEmergencyHandleActivation', 1, 'double', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
    } ...
  } ...
}'; 

if ~suppressObject 
    % Create bus objects in the MATLAB base workspace 
    Simulink.Bus.cellToObject(cellInfo) 
end 
