package com.sydac.mmrbem.tcms.common.data;

/**
 * This class hold data for HVAC Operating Mode Status. <br>
 * Note: Data can be hold as {@link Integer}
 */
public class VehicleHVACOpModeStatusDO
{

  private int hvacStatus;

  private int hvacFault;

  private int hvacIsolated;
  
  private int hvacReduced;

  public int getHvacStatus()
  {
    return hvacStatus;
  }

  public void setHvacStatus(int hvacStatus)
  {
    this.hvacStatus = hvacStatus;
  }

  public int getHvacFault()
  {
    return hvacFault;
  }

  public void setHvacFault(int hvacFault)
  {
    this.hvacFault = hvacFault;
  }

  public int getHvacIsolated()
  {
    return hvacIsolated;
  }

  public void setHvacIsolated(int hvacIsolated)
  {
    this.hvacIsolated = hvacIsolated;
  }
  
  public void setHvacReduced(int reduced)
  {
    this.hvacReduced = reduced;
  }
  
  public int getHvacReduced()
  {
    return hvacReduced;
  }
}