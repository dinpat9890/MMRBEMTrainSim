function cellInfo = TractionSystem_In_Vehicle_ElectricalSystem(varargin) 
% TRACTIONSYSTEM_IN_VEHICLE_ELECTRICALSYSTEM returns a cell array containing bus object information 
% 
% Optional Input: 'false' will suppress a call to Simulink.Bus.cellToObject 
%                 when the MATLAB file is executed. 
% The order of bus element attributes is as follows:
%   ElementName, Dimensions, DataType, SampleTime, Complexity, SamplingMode, DimensionsMode, Min, Max, DocUnits, Description 

suppressObject = false; 
if nargin == 1 && islogical(varargin{1}) && varargin{1} == false 
    suppressObject = true; 
elseif nargin > 1 
    error('Invalid input argument(s) encountered'); 
end 

cellInfo = { ... 
  { ... 
    'BD_TractionSystem_In_Vehicle_ElectricalSystem', ... 
    '', ... 
    '', ... 
    'Auto', ... 
    '-1', {... 
{'PowerAvailable', 1, 'boolean', -1, 'real', 'Sample', 'Fixed', [], [], '', sprintf('HV or DC Power available to the Traction boxes/motors')}; ...
{'ForwardTW', 1, 'boolean', -1, 'real', 'Sample', 'Fixed', [], [], '', sprintf('Forward Trainwire')}; ...
{'ReverseTW', 1, 'boolean', -1, 'real', 'Sample', 'Fixed', [], [], '', sprintf('Reverse Trainwire')}; ...
{'TractionActiveTW', 1, 'boolean', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'NoEmergencyBrakeActiveTW', 1, 'boolean', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'VCBState', 1, 'boolean', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'ContactorsFailure', 1, 'boolean', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
{'Bogie', 2, 'Bus: BD_TractionSystem_In_Vehicle_ElectricalSystem_Bogie', -1, 'real', 'Sample', 'Fixed', [], [], '', ''}; ...
    } ...
  } ...
}'; 

if ~suppressObject 
    % Create bus objects in the MATLAB base workspace 
    Simulink.Bus.cellToObject(cellInfo) 
end 
